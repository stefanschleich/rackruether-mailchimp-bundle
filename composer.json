{
    "name": "rackruether/mailchimp-bundle",
    "type": "contao-bundle",
    "description": "This extension connects a Contao form with the MailChimp API.",
    "keywords": [
        "contao",
        "mailchimp",
        "email",
        "marketing",
        "services"
    ],
    "homepage": "https://www.plusx.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Schleich",
            "email": "stefan@plusx.de",
            "homepage": "https://www.plusx.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.7",
        "oneup/mailchimp-api-v3": "^2.2",
        "patchwork/utf8": "^1.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "extra": {
        "contao-manager-plugin": "RackRuether\\MailChimpBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "RackRuether\\MailChimpBundle\\": "src"
        }
    },
    "prefer-stable": true,
    "support": {
        "email": "stefan@plusx.de",
        "issues": "https://github.com/stefanschleich/rackruether-mailchimp-bundle/issues",
        "source": "https://github.com/stefanschleich/rackruether-mailchimp-bundle"
    }
}